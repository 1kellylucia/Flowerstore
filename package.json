    "http-errors": "^1.7.1",
    "lodash": "^4.17.4",
    "mongod": "^2.0.0",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.6",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^4.2.0",
    "chai-things": "^0.2.0",
    "cross-env": "^5.2.0",
